Items: {
	BeholderBlade: {
		DisplayName: 凝視者之刃
		Tooltip:
			'''
			每揮舞的第三和四次會發射凝視光束
			暴擊時，敵人身上會產生虛空泡泡
			虛空泡泡裡會召喚眼睛攻擊敵人，並造成50%的傷害
			每次只能生成一個虛空泡泡
			'''
	}

	ChampionWarHorn: {
		DisplayName: 英靈戰號
		Tooltip:
			'''
			釋放追蹤的音波
			"CHARGE!"
			'''
	}

	ConcussiveInstrument: {
		DisplayName: 震盪性儀器
		Tooltip:
			'''
			打擊樂器攻擊時，會在敵人身上擲出一個錘子
			"這是……只是一把錘子?"
			'''
	}

	DeathsingerPotion: {
		DisplayName: 死亡歌者藥水
		Tooltip: 增加7%音波傷害和音波演奏速度
	}

	ExecutionersContract: {
		DisplayName: 劊子手協議
		Tooltip:
			'''
			牧師鐮刀的彈幕擊中敵人時也能獲得靈魂精華
			"把他們的靈魂帶給我…"
			'''
	}

	FanDonations: {
		DisplayName: 粉絲捐款
		Tooltip:
			'''
			所有樂器擊中敵人會產生爆炸，光輝大小取決於演奏的樂器和造成的暴擊傷害降低50%
			穿透性絃樂器攻擊每次擊中後速度增加，不再降低傷害
			絃樂器的彈幕在與實體方塊碰撞時產生的效果也能在敵人身上產生
			打擊樂器攻擊時，會在敵人身上擲出一個錘子
			電子樂器的彈幕速度大幅提升
			最大靈感增加5點
			"賣得並不好，但至少有人好心地提供了幫助…"
			'''
	}

	ImpulseAmplifier: {
		DisplayName: 脈衝揚聲器
		Tooltip:
			'''
			電子樂器的彈幕速度大幅提升
			"不，它不會比火箭飛得更快!"
			'''
	}

	InspirationRegenerationPotion: {
		DisplayName: 靈感恢復藥水
		Tooltip: 增加靈感恢復速度
	}

	JellyfishJam: {
		DisplayName: 水母狂想曲
		Tooltip: 釋放跳舞的水母刺穿敵人
	}

	LichWhip: {
		DisplayName: 笞魂刑具
		Tooltip:
			'''
			揮出由靈魂之火賦予力量的三重鞭
			敵人被鞭打後會流血
			流血的靈魂會釋放一根彩線治癒周圍的玩家
			"用來折磨死後靈魂的工具"
			'''
	}

	Loudener: {
		DisplayName: 擴音器
		Tooltip:
			'''
			管樂器和銅管樂器擊中敵人會產生金色光輝
			金色光輝大小取決於演奏的樂器
			爆炸造成的暴擊傷害降低50%
			"你有消音器，我有擴音器!"
			'''
	}

	LunateCharm: {
		DisplayName: 新月護身符
		Tooltip: 取消牧師盔甲帶來的非光輝傷害削減
	}

	Oneirophobia: {
		DisplayName: 恐夢症
		Tooltip:
			'''
			釋放多種類的幻影武器
			第一，二次攻擊時，武器會變成一把幻影劍
			第一次攻擊會產生強烈的衝擊波，第二次攻擊會產生三個衝擊波
			第三次攻擊時，武器的刀刃脫落並向前拋出
			然後武器會變成一把幻影鐮刀
			第四次攻擊時，鐮刀旋轉並釋放追蹤敵怪的光束
			第五次攻擊時，武器會變成幻影魔杖
			釋放不穩定的能量球，並在一段時間後爆炸
			第六次攻擊時，此武器會變成帶有鎖定敵怪的幻影鐳射槍
			不使用武器時，生成幻影僕從為你而戰
			"對夢和噩夢的非理性恐懼的表現"
			'''
	}

	RedCresent: {
		DisplayName: 緋紅新月
		Tooltip:
			'''
			釋放蝙蝠攻擊敵人，蝙蝠會回到你身邊
			蝙蝠造成的1/3傷害用來恢復吸血值
			右鍵釋放蝙蝠，蝙蝠會追蹤並治癒其他玩家
			每次消耗10點吸血值
			蝙蝠治癒<healAmount>點生命值，不會觸發治療效果
			但是，治療隊友會退還一些法力值給予治療者
			吸血值: <bloodSiphon>
			<legendaryModeOnly>在傳奇模式下使用需要生命值
			"蝙蝠的美妙歌謠"
			'''
	}

	MagicalHarp: {
		DisplayName: "{$ItemName.MagicalHarp}"
		Tooltip: ""
	}

	PickShapedPebble: {
		DisplayName: 鋤形卵石
		Tooltip:
			'''
			絃樂器的彈幕在與實體方塊碰撞時產生的效果也能在敵人身上產生
			"這是搖滾音樂!"
			'''
	}

	PocketEnergyStorm: {
		DisplayName: 可擕式能量風暴
		Tooltip:
			'''
			投擲一個不穩定的能量球，將花崗岩能量溢向目標
			被溢散的敵怪多受5%傷害
			按住左鍵讓能量球在你身邊環繞
			鬆開左鍵將你身邊所有的能量球向你滑鼠位置發射
			一次最多20個能量球
			不會增加疲憊值
			"賜予能量球新的意義"
			'''
	}

	RepurposedStarCaller: {
		DisplayName: 星際召喚器
		Tooltip:
			'''
			召喚監察者核心為你而戰
			火焰核心燃燒目標，生命核心毒害目標，寒冰核心造成冷凍
			"我們收到你的資訊了，現在有什麼指示?"
			'''
	}

	SealedContract: {
		DisplayName: 正式契約
		Tooltip:
			'''
			最大生命增加60
			治療法術會額外治療5點生命值
			牧師鐮刀的彈幕擊中敵人時也能獲得靈魂精華
			取消牧師盔甲帶來的非光輝傷害削減
			"正式達成協議!"
			'''
	}


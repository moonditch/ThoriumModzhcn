		DisplayName: 胃酸逆流
		Description: 增加10%傷害 和護甲穿透增加10
	}

	GourdGuarded: {
		DisplayName: 葫蘆保護
		Description: 增加20 防禦力
	}

	HandShieldCooldown: {
		DisplayName: 大手冷卻時間
		Description: 大手暫時抓不住他們……
	}

	InkLeaking: {
		DisplayName: 漏墨
		Description: 你的樂器在漏墨！
	}

	OrroBoroLeechCooldown: {
		DisplayName: 水蛭冷卻時間
		Description: 沒有螞蟥嗎?
	}

	Ulcer: {
		DisplayName: 潰瘍
		Description: 它在燃燒!
	}

	SerSemisonic: {
		DisplayName: Ser Semisonic詛咒靈魂（暫定名）
		Description: Mods.SpookyBardHealer.Buffs.SerSemisonic.Description（此物有10秒的時間）
	}

